{
  "name": "personal-profile-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:content-manager": "turbo run build --filter=content-manager",
    "dev": "turbo run dev",
    "dev:content-manager": "turbo run dev --filter=content-manager",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "start": "turbo run start",
    "start:content-manager": "turbo run start --filter=content-manager"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
